Host *
  User ashebanow
  IdentityAgent ~/.1password/agent.sock

Host github.com github
  User git
  IdentityAgent ~/.1password/agent.sock
  IdentityFile ~/.ssh/github_ed25519

Host gateway gateway.lan
  HostName 10.40.0.1

Host storage storage.lan
  HostName 10.40.0.226

Host virt1 virt1.lan
  HostName 10.50.0.11

Host virt2 virt2.lan
  HostName 10.50.0.14

